{
  "name": "MP3Juices Extension",
  "description": "Disable download manager / Blocking pop-up ads",
  "author": "KONO Shizuku",
  "icons": {
    "128": "assets/icons/icon128.png",
    "48": "assets/icons/icon48.png"
  },
  "version": "1.2.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background/js/block.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["http://mp3juices.to/search/*"],
      "css"    : [
        "content_scripts/css/dl_manager.css"
      ],
      "js"     : [
        "vendor/js/zepto.min.js",
        "vendor/js/underscore-min.js",
        "content_scripts/js/search.js"
      ],
      "run_at" : "document_start",
      "all_frames": false
    },
    {
      "matches": ["http://mp3juices.to/*"],
      "css"    : [
        "content_scripts/css/vaudix.css"
      ],
      "run_at" : "document_start",
      "all_frames": false
    }
  ],
  "permissions": [
    "http://mp3juices.to/*",
    "http://s.ad2387.com/*",
    "http://clkmon.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "vendor/js/*.map"
  ]
}
